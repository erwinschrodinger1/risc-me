#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x56d0b0fd5c70 .scope module, "memory_tb" "memory_tb" 2 3;
 .timescale -9 -12;
P_0x56d0b0fd5e00 .param/l "CLK_PERIOD" 0 2 6, +C4<00000000000000000000000000001010>;
v0x56d0b0feb6d0_0 .var "address", 7 0;
v0x56d0b0feb7b0_0 .var "clk", 0 0;
v0x56d0b0feb880_0 .var "input_data", 7 0;
v0x56d0b0feb980_0 .var "mem_write", 0 0;
v0x56d0b0feba50_0 .net "output_data", 7 0, v0x56d0b0feb550_0;  1 drivers
S_0x56d0b0fd5ea0 .scope module, "dut" "memory" 2 16, 3 1 0, S_0x56d0b0fd5c70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "mem_write";
    .port_info 2 /INPUT 8 "address";
    .port_info 3 /INPUT 8 "input_data";
    .port_info 4 /OUTPUT 8 "output_data";
v0x56d0b0f9b470_0 .net "address", 7 0, v0x56d0b0feb6d0_0;  1 drivers
v0x56d0b0feb1d0_0 .net "clk", 0 0, v0x56d0b0feb7b0_0;  1 drivers
v0x56d0b0feb290_0 .net "input_data", 7 0, v0x56d0b0feb880_0;  1 drivers
v0x56d0b0feb380 .array "mem", 255 0, 7 0;
v0x56d0b0feb440_0 .net "mem_write", 0 0, v0x56d0b0feb980_0;  1 drivers
v0x56d0b0feb550_0 .var "output_data", 7 0;
E_0x56d0b0fd4dc0 .event posedge, v0x56d0b0feb1d0_0;
    .scope S_0x56d0b0fd5ea0;
T_0 ;
    %wait E_0x56d0b0fd4dc0;
    %load/vec4 v0x56d0b0feb440_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x56d0b0feb290_0;
    %load/vec4 v0x56d0b0f9b470_0;
    %pad/u 10;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x56d0b0feb380, 0, 4;
T_0.0 ;
    %load/vec4 v0x56d0b0f9b470_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x56d0b0feb380, 4;
    %store/vec4 v0x56d0b0feb550_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56d0b0fd5c70;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56d0b0feb7b0_0, 0, 1;
    %vpi_call 2 27 "$dumpfile", "memory_tb.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56d0b0fd5c70 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x56d0b0feb6d0_0, 0, 8;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x56d0b0feb880_0, 0, 8;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x56d0b0feb6d0_0, 0, 8;
    %pushi/vec4 85, 0, 8;
    %store/vec4 v0x56d0b0feb880_0, 0, 8;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x56d0b0feb6d0_0, 0, 8;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x56d0b0feb880_0, 0, 8;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x56d0b0feb6d0_0, 0, 8;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x56d0b0feb6d0_0, 0, 8;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56d0b0feb980_0, 0, 1;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x56d0b0feb6d0_0, 0, 8;
    %delay 20000, 0;
    %delay 100000, 0;
    %vpi_call 2 65 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x56d0b0fd5c70;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x56d0b0feb7b0_0;
    %inv;
    %store/vec4 v0x56d0b0feb7b0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
